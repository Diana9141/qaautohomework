<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="QA Automation School Tests" verbose="1">
    <test name="All tests">
        <groups>
            <run>
                <include name="all"/>
            </run>
        </groups>
        <packages>
            <package name="tests.*"></package>
        </packages>
    </test>
</suite>